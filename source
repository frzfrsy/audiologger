local fake_module_scripts = {}

local function QNMFANN_fake_script() -- Fake Script: Workspace.AudioLoggah.MainFrame.TitleBar.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_TitleBar"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local dragHeader = script.Parent
	local frame = script.Parent.Parent
	
	local dragging, dragInput, dragStart, startPos
	
	dragHeader.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	dragHeader.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - dragStart
			frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
end
local function WZDV_fake_script() -- Fake Script: Workspace.AudioLoggah.MainFrame.TitleBar.GiveUp.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_GiveUp"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local button = script.Parent
	local clickTimeThreshold = 0.5
	local lastClickTime = 0
	
	button.MouseButton1Click:Connect(function()
		local currentTime = tick()
	
		if currentTime - lastClickTime <= clickTimeThreshold then
			script.Parent.Parent.Parent.Parent:Destroy()
		end
	
		lastClickTime = currentTime
	end)
end
local function HBYZ_fake_script() -- Fake Script: Workspace.AudioLoggah.MainFrame.AudioList.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
