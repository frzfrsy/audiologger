    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local function beep()
		script.Parent.Parent.TitleBar.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		script.Parent.Parent.TitleBar.TextColor3 = Color3.fromRGB(255, 255, 255)
		task.wait(0.05)
		script.Parent.Parent.TitleBar.BackgroundColor3 = Color3.fromRGB(255, 244, 151)
		script.Parent.Parent.TitleBar.TextColor3 = Color3.fromRGB(27, 42, 53)
		task.wait(0.05)
		script.Parent.Parent.TitleBar.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		script.Parent.Parent.TitleBar.TextColor3 = Color3.fromRGB(255, 255, 255)
		task.wait(0.05)
		script.Parent.Parent.TitleBar.BackgroundColor3 = Color3.fromRGB(255, 244, 151)
		script.Parent.Parent.TitleBar.TextColor3 = Color3.fromRGB(27, 42, 53)
	end
	
	
	script.Parent.ChildAdded:Connect(function()
		beep()
	end)
end
local function OFYEKP_fake_script() -- Fake Script: Workspace.AudioLoggah.MainFrame.InfoFrame.Frame.TXT.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_TXT"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local List = script.Parent.Parent.Parent.Parent.AudioList
	
	function createUI(assetid, ply, who)
		local Audio = Instance.new("TextButton")
		local stroke = Instance.new("UIStroke")
		local txtsetscale = Instance.new("UITextSizeConstraint")
	
		local str = assetid
		local numbers = {}
	
		for num in string.gmatch(str, "%d+") do
			table.insert(numbers, tonumber(num))
		end
	
		for _, number in ipairs(numbers) do
			local Asset = game:GetService("MarketplaceService"):GetProductInfo(number)
	
			Audio.Name = "Audio"
			Audio.Parent = List
			Audio.BackgroundColor3 = Color3.fromRGB(225, 207, 164)
			Audio.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Audio.BorderSizePixel = 0
			Audio.Size = UDim2.new(1, -4, 0, 25)
			Audio.Font = Enum.Font.Arial
			Audio.Text = Asset.Name
			Audio.TextColor3 = Color3.fromRGB(27, 42, 53)
			Audio.TextScaled = true
			Audio.TextTruncate = Enum.TextTruncate.AtEnd
			Audio.TextStrokeTransparency = 1
			Audio.MouseButton1Click:Connect(function()
				script.Parent.Parent.Parent.AudioInfo.ID.Text = number
				script.Parent.Parent.Parent.AudioInfo2.VALUE.Text = Asset.Creator.Name
				script.Parent.Parent.Parent.AudioInfo3.VALUE.Text = ply
				script.Parent.Parent.Parent.AudioInfo4.VALUE.Text = who
			end)
	
			stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			stroke.Color = Color3.fromRGB(17,17,17)
			stroke.Thickness = 1
			stroke.Transparency = 0.5
			stroke.Parent = Audio
			stroke.LineJoinMode = Enum.LineJoinMode.Round
	
			txtsetscale.MaxTextSize = 14
			txtsetscale.MinTextSize = 1
			txtsetscale.Parent = Audio
		end
	
		return Audio	
	end
	
	local Players = game:GetService("Players")
	local created = {}
	local button = script.Parent
	local loopRunning = false
	
	button.MouseButton1Click:Connect(function()
		if loopRunning then return end
		loopRunning = true
		button.Active = false
		button.Interactable = false
		button.AutoButtonColor = false
		button.Text = "Logging..."
		button.BackgroundColor3 = Color3.fromRGB(172, 158, 125)
	
		task.spawn(function()
			while true do
				wait(1)
				for _, v in pairs(game:GetDescendants()) do
					if v.Name == "Music" and not created[v.AssetId] then
						createUI(v.AssetId, v.PlaybackSpeed, v.Parent.Humanoid.DisplayName)
						created[v.AssetId] = true
					end
				end
			end
		end)
	end)
	
	
end
local function RKEU_fake_script() -- Fake Script: Workspace.AudioLoggah.MainFrame.InfoFrame.CopyID.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_CopyID"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local StarterGui = game:GetService("StarterGui")
	local everyClipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
	
	function toClipboard(txt)
		if everyClipboard then
			everyClipboard(tostring(txt))
			StarterGui:SetCore("SendNotification", {
				Title = "ID Copied";
				Text = "said Logger";
				Duration = 5;
			})
		else
			StarterGui:SetCore("SendNotification", {
				Title = "Problem";
				Text = "Your exploit does not have the ability to copy this, copy manual instead";
				Duration = 5;
			})
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		toClipboard(script.Parent.Parent.AudioInfo.ID.Text)
	end)
end
